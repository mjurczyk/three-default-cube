{
  "name": "three-default-cube",
  "version": "0.1.40",
  "description": "Game Development Toolkit for Three.js",
  "main": "./three-default-cube.js",
  "types": "./types/three-default-cube.d.ts",
  "files": [
    "three-default-cube.js",
    "three-default-cube.module.js",
    "types/**/*.d.ts"
  ],
  "directories": {
    "types": "./types"
  },
  "scripts": {
    "build:all": "npm run build:js && npm run build:types && npm pack",
    "build": "rollup --config ./rollup-config/cjs.config.js && npx -p typescript tsc three-default-cube.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "build:module": "rollup --config ./rollup-config/es.config.js && npx -p typescript tsc three-default-cube.module.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "build:js": "npm run build && npm run build:module",
    "build:types": "npx -p typescript tsc index.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "build:docs": "cd ./docs && hugo && cd ../",
    "docs": "cd docs && hugo server -D"
  },
  "keywords": [
    "three",
    "threejs",
    "webgl",
    "game",
    "game-development",
    "engine"
  ],
  "homepage": "https://defaultcube.wtlstudio.com/",
  "author": "wtlstudio",
  "repository": {
    "type": "git",
    "url": "https://github.com/mjurczyk/three-default-cube.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@rollup/plugin-babel": "5.3.0",
    "rollup": "2.52.3",
    "three-pathfinding": "0.14.1"
  },
  "peerDependencies": {
    "three": "^0.125.0",
    "@ionic-native/native-storage": "5.33.1",
    "@ionic-native/navigation-bar": "5.33.1",
    "@ionic-native/screen-orientation": "5.30.0",
    "@capacitor-community/admob": "1.2.6",
    "howler": "2.2.1",
    "postprocessing": "6.18.0",
    "troika-three-text": "0.35.0",
    "three-pathfinding": "0.14.1"
  }
}
